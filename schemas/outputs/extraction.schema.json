{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://empyrean.ai/schemas/outputs/extraction_v1.json",
  "title": "extraction_v1",
  "description": "Schema for simple key-value extractions.",
  "type": "object",
  "additionalProperties": false,
  "required": ["items"],
  "properties": {
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["key", "value"],
        "properties": {
          "key": {"type": "string"},
          "value": {"type": "string"}
        }
      }
    }
  }
}

